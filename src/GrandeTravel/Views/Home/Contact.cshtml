@model ContactViewModel
@{
    ViewBag.Title = "Grande Travel Contact";

}

<div id="bgIndex" class="container"> </div>

<div id="fb-root"></div>
<script>
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>

<div class="container">
    
    <div class="row">
        <div class="jumbotron col-md-12 col-lg-8 col-lg-offset-2" id="contactContent">
            <h2 class="text-capitalize text-center titanHeader">Grande Travel Contact</h2>
            <form asp-action="Contact" asp-controller="Home" method="post" role="form" asp-anti-forgery="true">
                <div>
                    <p>If Enquiring about Joining as a Travel Provider, please include your phone number and one of our staff will contact you. </p>
                </div>
                <div class="form-group">
                    <label asp-for="FromAddress"></label>
                    <input asp-for="FromAddress" class="form-control">
                    <span class="text-danger" asp-validation-for="FromAddress"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Subject"></label>
                    <input asp-for="Subject" class="form-control">
                    <span class="text-danger" asp-validation-for="Subject"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Body"></label>
                    <textarea asp-for="Body" class="form-control"></textarea>
                    <span class="text-danger" asp-validation-for="Body"></span>
                </div>
                <button type="submit" class="btn btn-default">Submit</button>
            </form>
        </div>
    </div>


    <div class="row">
        <h3 class="text-center text-capitalize">Our Social Media</h3>
        <div class="col-md-6 col-sm-6 socialMedia text-center" data-toggle="collapse" data-target="#twitter">
            <i class="fa fa-twitter ContactUsBtn" aria-hidden="true"></i>
            <div id="twitter" class="collapse">
                <a class="twitter-timeline" data-width="350" data-height="345" data-chrome="nofooter" data-theme="light" data-link-color="#2B7BB9" href="https://twitter.com/Australia">Tweets by Australia</a>
                <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
            </div>

        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 text-center socialMedia" data-toggle="collapse" data-target="#facebook1" id="facebook">

            <i class="fa fa-facebook ContactUsBtn" aria-hidden="true"></i>
            <div id="facebook1" class="collapse">

                <div class="fb-post" data-href="https://www.facebook.com/JoeBonno/posts/10208355704881478:0" data-width="200" data-show-text="true">
                    <blockquote cite="https://www.facebook.com/JoeBonno/posts/10208355704881478:0" class="fb-xfbml-parse-ignore">
                        <p>
                            Great Company, Amazing Experience.
                            Once in a Lifetime Experience
                        </p>Posted by <a href="https://www.facebook.com/JoeBonno">Joe E Silvia Bonifacio</a> on&nbsp;<a href="https://www.facebook.com/JoeBonno/posts/10208355704881478:0">Wednesday, March 29, 2017</a>
                    </blockquote>

                </div>
                &nbsp;
                <br />
               


            </div>


        </div>

